<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="tooth_view">
        <attr name="type">
            <enum name="triangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="rectangular" value="2" />
            <enum name="trapezoidal" value="3" />
        </attr>
        <attr name="location">
            <enum name="top" value="4" />
            <enum name="bottom" value="5" />
        </attr>
        <attr name="tooth_color" format="color"></attr>
    </declare-styleable>


</resources>
